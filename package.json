{
  "name": "@madeiramadeira/cavilha-react",
  "version": "1.0.0",
  "description": "A MadeiraMadeira React UI kit",
  "main": "lib/cavilha.js",
  "types": "lib/cavilha.d.ts",
  "author": "William Novak <williamnvk@gmail.com> (https://github.com/williamnvk)",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/williamnvk/cavilha-react"
  },
  "scripts": {
    "start": "webpack-dev-server --config config/webpack/server.js --open --mode development",
    "dist": "webpack --config config/webpack/dist.js --mode production",
    "build": "rm -rf lib/*.* && tsc",
    "prepublish": "npm run build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color --fix",
    "format": "prettier --write src/**/*.{ts,tsx,scss,css,json}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "(git-branch-is master && npm version minor || (git-branch-is dev && npm --no-git-tag-version version patch)"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "git add ."
    ]
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "dependencies": {
    "@madeiramadeira/cavilha": "git+https://github.com/williamnvk/cavilha.git#master",
    "@material-ui/core": "^4.9.8",
    "@material-ui/icons": "^4.9.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "webpack": "^4.42.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-export-default-from": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-syntax-highlighter": "^11.0.4",
    "@types/webpack": "4.41.8",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-env": "^1.7.0",
    "clean-terminal-webpack-plugin": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "css-loader": "^3.4.2",
    "csstype": "^2.6.10",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-airbnb-typescript": "7.2.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-config-react-app": "5.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "7.19.0",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "install": "^0.13.0",
    "lint-staged": "^10.0.8",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm": "^6.14.4",
    "prettier": "2.0.1",
    "prettier-eslint": "9.0.1",
    "prettier-eslint-cli": "5.0.0",
    "query-string": "^6.12.1",
    "react-router-dom": "^5.1.2",
    "react-syntax-highlighter": "^12.2.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack-auto-inject-version": "^1.2.2",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
    "lib",
    "README.md"
  ]
}
